{
  "openapiConfig": {
    "filePath": "OPENAPI_FILE_PATH"
  },
  "logger": {
    "level": "LOG_LEVEL"
  },
  "server": {
    "port": "SERVER_PORT",
     "request": {
      "payload": {
        "limit": "REQUEST_PAYLOAD_LIMIT"
      }
    },
    "response": {
      "compression": {
        "enabled": {
          "__name": "RESPONSE_COMPRESSION_ENABLED",
          "__format": "boolean"
        }
      }
    }
  },
  "db": {
    "type": "DB_TYPE",
    "host": "DB_HOST",
    "port": {
      "__name": "DB_PORT",
      "__format": "number"
    },
    "username": "DB_USERNAME",
    "password": "DB_PASSWORD",
    "enableSslAuth": {
      "__name": "DB_ENABLE_SSL_AUTH",
      "__format": "boolean"
    },
    "sslPaths": {
      "ca": "DB_CA_PATH",
      "key": "DB_KEY_PATH",
      "cert": "DB_CERT_PATH"
    },
    "database": "DB_NAME"
  }
}
